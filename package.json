{
  "devDependencies": {
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1"
  },
  "scripts": {
    "prepare": "husky install",
    "scriptname": "cmd",
    "start": "cd server && npm run start",
    "build": "cd client && npm run build",
    "install": "cd client && npm install --legacy-peer-deps && cd ../server && npm install"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
